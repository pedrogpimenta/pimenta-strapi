{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "Projects"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "string"
    },
    "Content": {
      "type": "richtext"
    },
    "ProjectLinks": {
      "type": "component",
      "repeatable": true,
      "component": "links.links"
    },
    "FeaturedImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "Images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "Published": {
      "type": "boolean",
      "default": false
    },
    "Order": {
      "type": "integer",
      "min": 1,
      "required": true
    }
  }
}
